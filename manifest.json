{
  "manifest_version": 3,
  "name": "LeetCode Notion Integration",
  "version": "1.0.0",
  "description": "A simple integration that helps update LeetCode Notion database about the user's performance",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "src/popup/popup.html"
  },
  "content_scripts": [
        {
            "matches": [
                "https://leetcode.com/problems/*"
            ],
            "js": ["src/content.js"]
        }
    ]
}